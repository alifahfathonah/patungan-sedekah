{
	"name": "ommu/1st-source",
	"description": "Ommu Platform First Source",
	"keywords": [
		"yii",
		"ommu",
		"platform",
		"ommu platform",
		"backoffice-theme",
		"cms",
		"module",
		"gii",
		"core",
		"users",
		"report",
		"support"
	],
	"homepage": "https://github.com/PutraSudaryanto/OmmuFirstSource",
	"type": "project",
	"license": "Apache-2.0",
	"support": {
		"issues": "https://github.com/PutraSudaryanto/OmmuFirstSource/issues",
		"wiki": "https://github.com/PutraSudaryanto/OmmuFirstSource/wiki",
		"source": "https://github.com/PutraSudaryanto/OmmuFirstSource"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PutraSudaryanto/OmmuFirstSource.git"
	},
	"authors": [
		{
			"name": "Putra Sudaryanto",
			"email": "putra@sudaryanto.id",
			"homepage": "https://putra.sudaryanto.id",
			"role": "Creator"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"oomphinc/composer-installers-extender": "^1.0",
		"yiisoft/yii": ">=1.1.14",
		"ommu/core": "~1.3",
		"ommu/gii": "~1.1",
		"ommu/ommu": "~1.0",
		"ommu/dora": "~0.2",
		"ommu/users":"~1.0",
		"ommu/support": "~1.0",
		"ommu/report": "~1.1",
		"ommu/article": "~1.3",
		"ommu/banner": "~1.4",

		"yiisoft/yii2": "~2.0.13",
		"yidas/yii2-bower-asset": "~2.0",
		"yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0"
	},
	"require-dev": {
        "yiisoft/yii2-debug": "~2.0.13",
        "phpunit/phpunit": "~5.7"
	},
	"config": {
		"process-timeout": 1800,
		"preferred-install": "dist",
		"vendor-dir": "protected/vendor"
	},
	"extra": {
		"process-timeout": 1800,
		"installer-types": [
			"ommu-module",
			"ommu-library",
			"ommu-theme",
			"ommu-extension"
		],
		"installer-paths": {
			"protected/libraries/{$name}/": [
				"type:ommu-library"
			],
			"themes/{$name}/": [
				"type:ommu-theme"
			],
			"protected/extensions/{$name}/": [
				"type:ommu-extension"
			]
		}
	}
}